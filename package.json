    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "webpack": "5.76.0"
  },
  "resolutions": {
    "colors": "1.4.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
