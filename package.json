    "ember-template-lint": "^2.21.0",
    "ember-try": "^2.0.0",
    "ember-wormhole": "0.6.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-ember": "^11.4.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
